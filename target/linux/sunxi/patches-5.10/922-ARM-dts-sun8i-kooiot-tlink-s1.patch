+ */
+
+/dts-v1/;
+#include "sun8i-v3.dtsi"
+
+#include <dt-bindings/gpio/gpio.h>
+#include <dt-bindings/input/input.h>
+
+/ {
+	model = "KooIoT TLink S1";
+	compatible = "kooiot,tlink-s1", "sochip,s3",
+		     "allwinner,sun8i-v3";
+
+	aliases {
+		serial0 = &uart0;
+	};
+
+	chosen {
+		stdout-path = "serial0:115200n8";
+	};
+
+	leds {
+		compatible = "gpio-leds";
+		pinctrl-names = "default";
+		pinctrl-0 = <&leds_pins>;
+
+		status {
+			label = "kooiot:green:status";
+			gpios = <&pio 6 6 GPIO_ACTIVE_HIGH>; /* PG6 */
+			linux,default-trigger = "heartbeat";
+		};
+		modem {
+			label = "kooiot:green:modem";
+			gpios = <&pio 6 7 GPIO_ACTIVE_HIGH>; /* PG7 */
+			default-state = "off";
+		};
+	};
+
+	gpio_keys {
+		compatible = "gpio-keys";
+		pinctrl-names = "default";
+		pinctrl-0 = <&keys_pins>;
+
+		reset {
+			label = "reset";
+			linux,code = <KEY_RESTART>;
+			gpios = <&pio 6 11 GPIO_ACTIVE_LOW>; /* PG11 */
+		};
+	};
+
+	gpio_export {
+		compatible = "gpio-export";
+		#size-cells = <0>;
+		// pinctrl-names = "default";
+		// pinctrl-0 = <&gpio_export_pins>;
+
+		pcie_reset {
+			gpio-export,name = "pcie_reset";
+			gpio-export,output=<0>;
+			gpios = <&pio 6 10 GPIO_ACTIVE_HIGH>; /* PG10 */
+		};
+		pcie_power {
+			gpio-export,name = "pcie_power";
+			gpio-export,output=<0>;
+			gpios = <&pio 6 8 GPIO_ACTIVE_LOW>; /* PG8 */
+		};
+	};
+
+	reg_vcc3v3: vcc3v3 {
+		compatible = "regulator-fixed";
+		regulator-name = "vcc3v3";
+		regulator-min-microvolt = <3300000>;
+		regulator-max-microvolt = <3300000>;
+	};
+
+	reg_vcc1v8: vcc1v8 {
+		compatible = "regulator-fixed";
+		regulator-name = "vcc1v8";
+		regulator-min-microvolt = <1800000>;
+		regulator-max-microvolt = <1800000>;
+		gpio = <&pio 6 9 GPIO_ACTIVE_HIGH>; /* PG9 */
+		enable-active-high;
+	};
+};
+
+&mmc0 {
+	vmmc-supply = <&reg_vcc3v3>;
+	bus-width = <4>;
+	cd-gpios = <&pio 5 6 GPIO_ACTIVE_LOW>; /* PF6 */
+	status = "okay";
+};
+
+&mmc2 {
+	compatible = "allwinner,sun8i-s3-emmc";
+	pinctrl-names = "default";
+	pinctrl-0 = <&mmc2_pins>;
+	vmmc-supply = <&reg_vcc3v3>;
+	vqmmc-supply = <&reg_vcc3v3>;
+	bus-width = <8>;
+	non-removable;
+	status = "okay";
+};
+
+&pio {
+	mmc2_pins: mmc2-pins {
+		pins = "PC0", "PC1", "PC2",
+			   "PC3", "PC4", "PC5",
+			   "PC6", "PC7", "PC8",
+			   "PC9", "PC10";
+		function = "mmc2";
+		drive-strength = <40>;
+		bias-pull-up;
+	};
+
+	leds_pins: leds-pins {
+		pins = "PG6", "PG7";
+		function = "gpio_out";
+	};
+
+	keys_pins: keys-pins {
+		pins = "PG11";
+		function = "gpio_in";
+	};
+
+	gpio_export_pins: gpio-export-pins {
+		pins = "PG8", "PG10";
+		function = "gpio_out";
+	};
+};
+
+&uart0 {
+	pinctrl-0 = <&uart0_pb_pins>;
+	pinctrl-names = "default";
+	status = "okay";
+};
+
+&i2c0 {
+    status = "okay";
+    sd3077: sd3077@32 {
+        compatible = "whwave,sd3078";
+        reg = <0x32>;
+        status = "okay";
+    };
+    at24c02: eeprom@50 {
+        compatible = "atmel,24c02";
+        reg = <0x50>;
+        pagesize = <16>;
+        status = "okay";
+    };
+};
+
+&emac {
+	phy-handle = <&int_mii_phy>;
+	phy-mode = "mii";
+    status = "okay";
+};
+
+&usb_otg {
+	dr_mode = "host";
+	status = "okay";
+};
+
+&usbphy {
+	usb0_vbus-supply = <&reg_vcc3v3>;
+	status = "okay";
+};
+
+&ehci0 {
+	status = "okay";
+};
+
+&ohci0 {
+	status = "okay";
+};
