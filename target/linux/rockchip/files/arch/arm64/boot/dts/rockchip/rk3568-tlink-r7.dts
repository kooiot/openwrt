/dts-v1/;
#include <dt-bindings/gpio/gpio.h>
#include <dt-bindings/pwm/pwm.h>
#include <dt-bindings/input/input.h>
#include <dt-bindings/pinctrl/rockchip.h>
#include "rk3568.dtsi"

/ {
	model = "ThingsLink R7";
	compatible = "kooiot,tlink-r7", "rockchip,rk3568";

	aliases {
		mmc0 = &sdmmc0;
		mmc1 = &sdhci;
		mmc2 = &sdmmc2;

		led-boot = &status_led;
		led-failsafe = &status_led;
		led-upgrade = &status_led;

		ethernet0 = &gmac0;
		ethernet1 = &gmac1;
		ethernet2 = &rtl8111s_1;
		ethernet3 = &rtl8111s_2;

		serial0 = &uart2;
		serial1 = &uart0;
		serial2 = &uart3;
		serial3 = &uart4;
		serial4 = &uart5;
		serial5 = &uart7;
		serial6 = &uart9;
		serial30 = &uart8;

		rtc0 = &sd3077;
	};

	chosen: chosen {
		stdout-path = "serial0:115200n8";
	};

	dc_12v: dc-12v {
		compatible = "regulator-fixed";
		regulator-name = "dc_12v";
		regulator-always-on;
		regulator-boot-on;
		regulator-min-microvolt = <12000000>;
		regulator-max-microvolt = <12000000>;
	};

	vcc2v5_sys: vcc2v5-ddr {
		compatible = "regulator-fixed";
		regulator-name = "vcc2v5-sys";
		regulator-always-on;
		regulator-boot-on;
		regulator-min-microvolt = <2500000>;
		regulator-max-microvolt = <2500000>;
		vin-supply = <&vcc3v3_sys>;
	};

	vccio_flash: vccio-flash {
		compatible = "regulator-fixed";
		regulator-name = "vccio_flash";
		regulator-always-on;
		regulator-boot-on;
		regulator-min-microvolt = <1800000>;
		regulator-max-microvolt = <1800000>;
		vin-supply = <&vcc_3v3>;
	};

	pcie30_avdd0v9: pcie30-avdd0v9 {
		compatible = "regulator-fixed";
		regulator-name = "pcie30_avdd0v9";
		regulator-always-on;
		regulator-boot-on;
		regulator-min-microvolt = <900000>;
		regulator-max-microvolt = <900000>;
		vin-supply = <&vcc3v3_sys>;
	};

	pcie30_avdd1v8: pcie30-avdd1v8 {
		compatible = "regulator-fixed";
		regulator-name = "pcie30_avdd1v8";
		regulator-always-on;
		regulator-boot-on;
		regulator-min-microvolt = <1800000>;
		regulator-max-microvolt = <1800000>;
		vin-supply = <&vcc3v3_sys>;
	};

	pcie20_3v3: gpio-regulator {
		compatible = "regulator-fixed";
		regulator-name = "pcie20_3v3";
		regulator-always-on;
		regulator-boot-on;
		regulator-min-microvolt = <3300000>;
		regulator-max-microvolt = <3300000>;
		// vin-supply = <&vcc5v0_sys>;
		// pinctrl-names = "default";
		// pinctrl-0 = <&pcie20_power_pin>;
		// gpios = <&gpio0 RK_PD4 GPIO_ACTIVE_HIGH>;
		// gpios-states = <0x1>;
		// states = <100000 0x0 3300000 0x1>;
	};

	vcc3v3_bu: vcc3v3-bu {
		compatible = "regulator-fixed";
		regulator-name = "vcc3v3_bu";
		regulator-always-on;
		regulator-boot-on;
		regulator-min-microvolt = <3300000>;
		regulator-max-microvolt = <3300000>;
		vin-supply = <&vcc5v0_sys>;
	};

	vcc3v3_sys: vcc3v3-sys {
		compatible = "regulator-fixed";
		regulator-name = "vcc3v3_sys";
		regulator-always-on;
		regulator-boot-on;
		regulator-min-microvolt = <3300000>;
		regulator-max-microvolt = <3300000>;
		vin-supply = <&dc_12v>;
	};

	vcc5v0_sys: vcc5v0-sys {
		compatible = "regulator-fixed";
		regulator-name = "vcc5v0_sys";
		regulator-always-on;
		regulator-boot-on;
		regulator-min-microvolt = <5000000>;
		regulator-max-microvolt = <5000000>;
		vin-supply = <&dc_12v>;
	};

	vcc3v3_usb: vcc3v3-usb-regulator {
		compatible = "regulator-fixed";
		regulator-name = "vcc3v3_usb";
		regulator-boot-on;
		regulator-always-on;
		regulator-min-microvolt = <3300000>;
		regulator-max-microvolt = <3300000>;
		vin-supply = <&dc_12v>;
		// DNP - R8328 -- controlled by GPIO
		// pinctrl-names = "default";
		// pinctrl-0 = <&vcc3v3_usb_en>;
		// gpio = <&gpio0 RK_PC7 GPIO_ACTIVE_HIGH>;
		// enable-active-high;
	};

	vcc5v0_host2: vcc5v0-host2-regulator {
		compatible = "regulator-fixed";
		regulator-name = "vcc5v0_host2";
		regulator-boot-on;
		regulator-always-on;
		vin-supply = <&vcc5v0_sys>;
		// DNP - R2511 - GPIO conflict with SPI0_MISO_M0
		// pinctrl-names = "default";
		// pinctrl-0 = <&vcc5v0_host2_en>;
		// gpio = <&gpio0 RK_PC5 GPIO_ACTIVE_HIGH>;
		// enable-active-high;
	};

	vcc5v0_host3: vcc5v0-host2-regulator {
		compatible = "regulator-fixed";
		regulator-name = "vcc5v0_host3";
		regulator-boot-on;
		regulator-always-on;
		regulator-min-microvolt = <5000000>;
		regulator-max-microvolt = <5000000>;
		vin-supply = <&vcc5v0_sys>;
		// DNP - R2516 - GPIO conflict with SPI0_CS0_M0
		// pinctrl-names = "default";
		// pinctrl-0 = <&vcc5v0_host3_en>;
		// gpio = <&gpio0 RK_PC6 GPIO_ACTIVE_HIGH>;
		// enable-active-high;
	};

	vcc5v0_vga: vcc5v0-vga-regulator {
		compatible = "regulator-fixed";
		regulator-name = "vcc5v0_vga";
		regulator-boot-on;
		regulator-always-on;
		regulator-min-microvolt = <5000000>;
		regulator-max-microvolt = <5000000>;
		vin-supply = <&vcc5v0_sys>;
		pinctrl-names = "default";
		pinctrl-0 = <&vcc5v0_vga_en>;
		gpio = <&gpio0 RK_PD6 GPIO_ACTIVE_HIGH>;
		enable-active-high;
	};

	vcc3v3_vga: vcc3v3-vga-regulator {
		compatible = "regulator-fixed";
		regulator-name = "vcc3v3_vga";
		regulator-boot-on;
		regulator-always-on;
		regulator-min-microvolt = <3300000>;
		regulator-max-microvolt = <3300000>;
		vin-supply = <&vcc5v0_vga>;
	};

	pcie20_pi6c_oe: pcie20-pi6c-oe-regulator {
		compatible = "regulator-fixed";
		regulator-name = "pcie20_pi6c_oe";
		regulator-always-on;
		pinctrl-names = "default";
		pinctrl-0 = <&pcie20_pi6c_oe_en>;
		gpio = <&gpio1 RK_PB3 GPIO_ACTIVE_LOW>;
		//enable-active-high;
	};

	sdio_pwrseq: sdio-pwrseq {
		compatible = "mmc-pwrseq-simple";
		clocks = <&rk809 1>;
		clock-names = "ext_clock";
		pinctrl-names = "default";
		pinctrl-0 = <&wifi_enable_h>;
		/*
			* On the module itself this is one of these (depending
			* on the actual card populated):
			* - SDIO_RESET_L_WL_REG_ON
			* - PDN (power down when low)
		*/
		reset-gpios = <&gpio3 RK_PD5 GPIO_ACTIVE_LOW>;
		post-power-on-delay-ms = <100>;
	};

	leds {
		compatible = "gpio-leds";
		status = "okay";

		status_led: status-led {
			label = "kooiot:green:status";
			linux,default-trigger = "heartbeat";
			gpios = <&gpio0 RK_PD5 GPIO_ACTIVE_LOW>;
			pinctrl-names = "default";
			pinctrl-0 = <&led_status_pin>;
		};

		csq {
			label = "kooiot:green:csq";
			gpios = <&gpio3 RK_PA0 GPIO_ACTIVE_LOW>;
			default-state = "off";
			pinctrl-names = "default";
			pinctrl-0 = <&led_csq_pin>;
		};

		modem {
			label = "kooiot:green:modem";
			gpios = <&gpio3 RK_PA1 GPIO_ACTIVE_LOW>;
			default-state = "off";
			pinctrl-names = "default";
			pinctrl-0 = <&led_modem_pin>;
		};

		cloud {
			label = "kooiot:green:cloud";
			gpios = <&gpio3 RK_PA2 GPIO_ACTIVE_LOW>;
			default-state = "off";
			pinctrl-names = "default";
			pinctrl-0 = <&led_cloud_pin>;
		};
	};

	gpio_keys {
		compatible = "gpio-keys";
		pinctrl-names = "default";
		pinctrl-0 = <&key_reset_pin>;

		reset {
			label = "reset";
			linux,code = <KEY_RESTART>;
			gpios = <&gpio0 RK_PA5 GPIO_ACTIVE_LOW>;
		};
	};

	gpio_export {
		compatible = "gpio-export";
		#size-cells = <0>;

		/*
		pcie_power {
			gpio-export,name = "pcie_power";
			gpio-export,output=<1>;
			gpios = <&gpio0 RK_PC7 GPIO_ACTIVE_HIGH>;
			pinctrl-names = "default";
			pinctrl-0 = <&pcie_power_pin>;
		};
		*/

		pcie_reset {
			gpio-export,name = "pcie_reset";
			gpio-export,output=<0>;
			gpios = <&gpio0 RK_PC4 GPIO_ACTIVE_HIGH>;
			pinctrl-names = "default";
			pinctrl-0 = <&pcie_reset_pin>;
		};

		pcie_on_off {
			gpio-export,name = "pcie_on_off";
			gpio-export,output=<1>;
			gpios = <&gpio0 RK_PC3 GPIO_ACTIVE_HIGH>;
			pinctrl-names = "default";
			pinctrl-0 = <&pcie_on_off_pin>;
		};

		pcie_disable {
			gpio-export,name = "pcie_disable";
			gpio-export,output=<1>;
			gpios = <&gpio0 RK_PC2 GPIO_ACTIVE_LOW>;
			pinctrl-names = "default";
			pinctrl-0 = <&pcie_disable_pin>;
		};

		pcie_card_power_off {
			gpio-export,name = "pcie_card_power_off";
			gpio-export,output=<1>;
			gpios = <&gpio1 RK_PA1 GPIO_ACTIVE_LOW>;
			pinctrl-names = "default";
			pinctrl-0 = <&pcie_card_power_off_pin>;
		};

		pcie_5g_working_led {
			gpio-export,name = "pcie_5g_working_led";
			gpio-export,output=<1>;
			gpios = <&gpio1 RK_PA0 GPIO_ACTIVE_LOW>;
			pinctrl-names = "default";
			pinctrl-0 = <&led_5g_working_pin>;
		};

		pcie20_power {
			gpio-export,name = "pcie20_power";
			gpio-export,output=<1>;
			gpios = <&gpio0 RK_PD4 GPIO_ACTIVE_HIGH>;
			pinctrl-names = "default";
			pinctrl-0 = <&pcie20_power_pin>;
		};

		pcie20_reset {
			gpio-export,name = "pcie20_reset";
			gpio-export,output=<0>;
			gpios = <&gpio2 RK_PD7 GPIO_ACTIVE_HIGH>;
			pinctrl-names = "default";
			pinctrl-0 = <&pcie20_reset_pin>;
		};

		pcie20_on_off {
			gpio-export,name = "pcie20_on_off";
			gpio-export,output=<1>;
			gpios = <&gpio3 RK_PC1 GPIO_ACTIVE_HIGH>;
			pinctrl-names = "default";
			pinctrl-0 = <&pcie20_on_off_pin>;
		};

		pcie20_disable {
			gpio-export,name = "pcie20_disable";
			gpio-export,output=<1>;
			gpios = <&gpio3 RK_PB5 GPIO_ACTIVE_LOW>;
			pinctrl-names = "default";
			pinctrl-0 = <&pcie20_disable_pin>;
		};

		pcie20_card_power_off {
			gpio-export,name = "pcie20_card_power_off";
			gpio-export,output=<1>;
			gpios = <&gpio3 RK_PB6 GPIO_ACTIVE_LOW>;
			pinctrl-names = "default";
			pinctrl-0 = <&pcie20_card_power_off_pin>;
		};

		/* SATA ACTn/GPIO9 for led */
		sata_en {
			gpio-export,name = "sata_en";
			// gpio-export,output=<1>; // Input GPIO
			gpios = <&gpio0 RK_PA6 GPIO_ACTIVE_HIGH>;
			pinctrl-names = "default";
			pinctrl-0 = <&m2_sata_en_pin>;
		};

		wk2xxx_reset_1 {
			gpio-export,name = "wk2xxx_reset_1";
			gpio-export,output=<1>;
			gpios = <&gpio4 RK_PC7 GPIO_ACTIVE_LOW>;
			pinctrl-names = "default";
			pinctrl-0 = <&wk_reset_pin>;
		};

		do_1_en {
			gpio-export,name = "do_1_en";
			gpio-export,output=<1>;
			gpios = <&gpio4 RK_PD1 GPIO_ACTIVE_LOW>;
			pinctrl-names = "default";
			pinctrl-0 = <&do1_en_pin>;
		};

		do_1 {
			gpio-export,name = "do_1";
			gpio-export,output=<0>;
			gpios = <&gpio3 RK_PA3 GPIO_ACTIVE_LOW>;
			pinctrl-names = "default";
			pinctrl-0 = <&do1_pin>;
		};

		do_2_en {
			gpio-export,name = "do_2_en";
			gpio-export,output=<1>;
			gpios = <&gpio4 RK_PD2 GPIO_ACTIVE_LOW>;
			pinctrl-names = "default";
			pinctrl-0 = <&do2_en_pin>;
		};

		do_2 {
			gpio-export,name = "do_2";
			gpio-export,output=<0>;
			gpios = <&gpio3 RK_PA4 GPIO_ACTIVE_LOW>;
			pinctrl-names = "default";
			pinctrl-0 = <&do2_pin>;
		};

		do_3_en {
			gpio-export,name = "do_3_en";
			gpio-export,output=<1>;
			gpios = <&gpio0 RK_PA0 GPIO_ACTIVE_LOW>;
			pinctrl-names = "default";
			pinctrl-0 = <&do3_en_pin>;
		};

		do_3 {
			gpio-export,name = "do_3";
			gpio-export,output=<0>;
			gpios = <&gpio3 RK_PA5 GPIO_ACTIVE_LOW>;
			pinctrl-names = "default";
			pinctrl-0 = <&do3_pin>;
		};

		do_4_en {
			gpio-export,name = "do_4_en";
			gpio-export,output=<1>;
			gpios = <&gpio3 RK_PA7 GPIO_ACTIVE_LOW>;
			pinctrl-names = "default";
			pinctrl-0 = <&do4_en_pin>;
		};

		do_4 {
			gpio-export,name = "do_4";
			gpio-export,output=<0>;
			gpios = <&gpio3 RK_PA6 GPIO_ACTIVE_LOW>;
			pinctrl-names = "default";
			pinctrl-0 = <&do4_pin>;
		};

		di_1 {
			gpio-export,name = "di_1";
			gpio-export,direction_may_change = <1>;
			gpios = <&gpio1 RK_PA4 GPIO_ACTIVE_LOW>;
			pinctrl-names = "default";
			pinctrl-0 = <&di1_pin>;
		};

		di_2 {
			gpio-export,name = "di_2";
			gpio-export,direction_may_change = <1>;
			gpios = <&gpio1 RK_PA6 GPIO_ACTIVE_LOW>;
			pinctrl-names = "default";
			pinctrl-0 = <&di2_pin>;
		};

		di_3 {
			gpio-export,name = "di_3";
			gpio-export,direction_may_change = <1>;
			gpios = <&gpio1 RK_PA7 GPIO_ACTIVE_LOW>;
			pinctrl-names = "default";
			pinctrl-0 = <&di3_pin>;
		};

		di_4 {
			gpio-export,name = "di_4";
			gpio-export,direction_may_change = <1>;
			gpios = <&gpio1 RK_PB7 GPIO_ACTIVE_LOW>;
			pinctrl-names = "default";
			pinctrl-0 = <&di4_pin>;
		};
	};
};

&combphy0 {
	status = "okay";
};

&combphy1 {
	status = "okay";
};

&combphy2 {
	status = "okay";
};

&cpu0 {
	cpu-supply = <&vdd_cpu>;
};

&cpu1 {
	cpu-supply = <&vdd_cpu>;
};

&cpu2 {
	cpu-supply = <&vdd_cpu>;
};

&cpu3 {
	cpu-supply = <&vdd_cpu>;
};

&cpu_thermal {
	trips {
		cpu_hot: cpu_hot {
			temperature = <55000>;
			hysteresis = <2000>;
			type = "active";
		};
	};

	/* cooling-maps {
		map1 {
			trip = <&cpu_hot>;
			// cooling-device = <&fan THERMAL_NO_LIMIT THERMAL_NO_LIMIT>;
		};
	}; */
};

&gmac0 {
	assigned-clocks = <&cru SCLK_GMAC0_RX_TX>, <&cru SCLK_GMAC0>;
	assigned-clock-parents = <&cru SCLK_GMAC0_RGMII_SPEED>;
	assigned-clock-rates = <0>, <125000000>;
	clock_in_out = "output";
	phy-handle = <&rgmii_phy0>;
	phy-mode = "rgmii-id";
	pinctrl-names = "default";
	pinctrl-0 = <&gmac0_miim
		     &gmac0_tx_bus2
		     &gmac0_rx_bus2
		     &gmac0_rgmii_clk
		     &gmac0_rgmii_bus>;
	tx_delay = <0x3c>;
	rx_delay = <0x2f>;
	snps,reset-gpio = <&gpio2 RK_PD3 GPIO_ACTIVE_LOW>;
	snps,reset-active-low;
	/* Reset time is 20ms, 100ms for rtl8211f */
	snps,reset-delays-us = <0 20000 100000>;
	status = "okay";
};

&gmac1 {
	assigned-clocks = <&cru SCLK_GMAC1_RX_TX>, <&cru SCLK_GMAC1>;
	assigned-clock-parents = <&cru SCLK_GMAC1_RGMII_SPEED>;
	assigned-clock-rates = <0>, <125000000>;
	clock_in_out = "output";
	phy-handle = <&rgmii_phy1>;
	phy-mode = "rgmii-id";
	pinctrl-names = "default";
	pinctrl-0 = <&gmac1m1_miim
		     &gmac1m1_tx_bus2
		     &gmac1m1_rx_bus2
		     &gmac1m1_rgmii_clk
		     &gmac1m1_rgmii_bus>;
	tx_delay = <0x4f>;
	rx_delay = <0x26>;
	snps,reset-gpio = <&gpio2 RK_PD1 GPIO_ACTIVE_LOW>;
	snps,reset-active-low;
	/* Reset time is 20ms, 100ms for rtl8211f */
	snps,reset-delays-us = <0 20000 100000>;
	status = "okay";
};

&i2c0 {
	status = "okay";

	vdd_cpu: regulator@1c {
		compatible = "tcs,tcs4525";
		reg = <0x1c>;
		vin-supply = <&vcc5v0_sys>;
		regulator-compatible = "fan53555-reg";
		regulator-name = "vdd_cpu";
		regulator-min-microvolt = <712500>;
		regulator-max-microvolt = <1390000>;
		regulator-ramp-delay = <2300>;
		fcs,suspend-voltage-selector = <1>;
		regulator-boot-on;
		regulator-always-on;

		regulator-state-mem {
			regulator-off-in-suspend;
		};
	};

	rk809: pmic@20 {
		compatible = "rockchip,rk809";
		reg = <0x20>;
		interrupt-parent = <&gpio0>;
		interrupts = <RK_PA3 IRQ_TYPE_LEVEL_LOW>;
		assigned-clocks = <&cru I2S1_MCLKOUT_TX>;
		assigned-clock-parents = <&cru CLK_I2S1_8CH_TX>;
		#clock-cells = <1>;
		clock-names = "mclk";
		clocks = <&cru I2S1_MCLKOUT_TX>;
		pinctrl-names = "default";
		pinctrl-0 = <&pmic_int &pmic_pins>;
		rockchip,system-power-controller;
		vcc1-supply = <&vcc3v3_sys>;
		vcc2-supply = <&vcc3v3_sys>;
		vcc3-supply = <&vcc3v3_sys>;
		vcc4-supply = <&vcc3v3_sys>;
		vcc5-supply = <&vcc3v3_sys>;
		vcc6-supply = <&vcc3v3_sys>;
		vcc7-supply = <&vcc3v3_sys>;
		vcc8-supply = <&vcc3v3_sys>;
		vcc9-supply = <&vcc3v3_sys>;
		wakeup-source;

		rtc {
			status = "disabled";
		};

		regulators {
			vdd_logic: DCDC_REG1 {
				regulator-name = "vdd_logic";
				regulator-always-on;
				regulator-boot-on;
				regulator-init-microvolt = <900000>;
				regulator-initial-mode = <0x2>;
				regulator-min-microvolt = <500000>;
				regulator-max-microvolt = <1350000>;
				regulator-ramp-delay = <6001>;

				regulator-state-mem {
					regulator-off-in-suspend;
				};
			};

			vdd_gpu: DCDC_REG2 {
				regulator-always-on;
				regulator-boot-on;
				regulator-name = "vdd_gpu";
				regulator-init-microvolt = <900000>;
				regulator-initial-mode = <0x2>;
				regulator-min-microvolt = <500000>;
				regulator-max-microvolt = <1350000>;
				regulator-ramp-delay = <6001>;

				regulator-state-mem {
					regulator-off-in-suspend;
				};
			};

			vcc_ddr: DCDC_REG3 {
				regulator-name = "vcc_ddr";
				regulator-always-on;
				regulator-boot-on;
				regulator-initial-mode = <0x2>;

				regulator-state-mem {
					regulator-on-in-suspend;
				};
			};

			vdd_npu: DCDC_REG4 {
