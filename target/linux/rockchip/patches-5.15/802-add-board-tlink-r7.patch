+		     &gmac1m1_rgmii_clk
+		     &gmac1m1_rgmii_bus
+		     &gmac1m1_clkinout
+		     &gmac1_rst>;
+	// tx_delay = <0x4a>; // Dev board
+	// rx_delay = <0x2c>;
+	tx_delay = <0x45>;
+	rx_delay = <0x28>;
+	snps,reset-gpio = <&gpio2 RK_PD1 GPIO_ACTIVE_LOW>;
+	snps,reset-active-low;
